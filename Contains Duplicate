class Solution {
    public static boolean containsDuplicate(int[] nums) {
        Arrays.sort(nums); //sort the array using inbuilt sort 

        for (int i =0 ; i<nums.length-1 ; i++){
            if (nums[i] == nums[i+1]){    //check if adjacent elements are same 
                return true;
            }
        }
        return false;
    }
}
   
    
